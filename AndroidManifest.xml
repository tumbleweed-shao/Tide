<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tide"
    android:versionCode="1"
    android:versionName="1.0"
    android:installLocation="auto"
    >

    <uses-sdk
        android:minSdkVersion="4"
        android:targetSdkVersion="4" />
	<uses-permission android:name="android.permission.INTERNET"/> 
 	<uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" /> 
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_new"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.tide.TideMainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.tide.About"></activity>
         <activity android:name="com.adfeiwo.banner.WebViewActivity" 
             android:configChanges="keyboardHidden|orientation" />
        
        <service android:name="com.tide.service.TideService"></service>
        <receiver android:name="com.tide.TideMainActivity$ManagerReceiver">
            <intent-filter >
                <action android:name="android.intent.action.SENDTIDETOACTIVITY" />
            </intent-filter>
        </receiver>
        <receiver android:name="com.adfeiwo.banner.AdReceiver">
            <intent-filter>
				<action android:name="android.intent.action.PACKAGE_ADDED" />
				<data android:scheme="package" />
			</intent-filter>
        </receiver>
        
    </application>
</manifest>
